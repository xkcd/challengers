    "webpack-dev-server": "^3.1.9",
    "which-polygon": "^2.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "d3": "^5.7.0",
    "d3-geo": "^1.11.1",
    "d3-selection": "^1.3.2",
    "d3-zoom": "^1.7.3",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "rbush": "^2.0.2",
    "topojson": "^3.0.2"
  }
