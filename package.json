{
  "name": "xkcd-map",
  "version": "1.0.0",
  "description": "A zoomable map of US 2018 election challengers",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run build:data && npm run build:layout && npm run build:web",
    "build:web": "webpack --mode production",
    "build:data": "cd data; ./generate.sh",
    "build:layout": "scripts/bakeLayout.js > ./dist/map-data.json",
    "start:dev": "webpack-dev-server --mode=development -d -w"
  },
