    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@mapbox/geojson-area": "^0.2.2",
    "@mapbox/geojson-merge": "^1.0.2",
    "babel-loader": "^8.0.4",
    "csv-parser": "^2.1.0",
    "d3-geo-projection": "^2.0.0",
    "flatbush": "^3.1.0",
    "geojson": "^0.5.0",
    "get-stdin": "^6.0.0",
    "mapshaper": "^0.4.99",
    "ndjson-cli": "^0.3.0",
    "neat-csv": "^3.0.0",
    "puppeteer": "^1.9.0",
    "shapefile": "^0.6.1",
    "topojson-client": "^3.0.0",
